<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name>JavaWeb</display-name>

    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>com.sdw.web.TestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/servlet/test</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Helloworld2</servlet-name>
        <servlet-class>com.sdw.web.Helloworld2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Helloworld2</servlet-name>
        <url-pattern>/Helloworld</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>HelloServer</servlet-name>
        <servlet-class>com.sdw.web.server.HelloServer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloServer</servlet-name>
        <url-pattern>/server/HelloServer</url-pattern>
    </servlet-mapping>
</web-app>